import java.util.*;

public class AlphaBeta {
     public static void main(String []args){
        Scanner sc = new Scanner(System.in);
        int a = sc.nextInt();
        int b = sc.nextInt();
        int count=0;
        do {
            a = a+a+a;
            b = b+b;
            count++;
        }while(a<=b);
        
        System.out.println(count);
     }
}