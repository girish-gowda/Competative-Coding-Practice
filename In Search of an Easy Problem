import java.util.*;

public class AlphaBeta{

     public static void main(String[] args){
		java.util.Scanner sc = new java.util.Scanner(System.in);
        int n = sc.nextInt();
        boolean flag = true;
        for(int i=0;i<n;i++){
            if(sc.nextInt()==1){
                flag = false;
                break;
            }
        }
        
        if(flag){
            System.out.println("EASY");
        }else{
            System.out.println("HARD");
        }
     }
}